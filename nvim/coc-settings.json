{
  "diagnostic.messageTarget": "float",
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "css", "typescript", "typescriptreact", "json", "Markdown", "scss", "yaml", "elm", "ruby"],
  "eslint.run": "onSave",
  "eslint.filetypes":["javascript","javascriptreact", "typescript", "typescriptreact"],
  "tsserver.locale": "ja-JP",
  "solargraph.commandPath": "/Users/roy/.rbenv/shims/solargraph",
  "solargraph.hover": true,
  "solargraph.completion": true,
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "html.enable": true,
  "html.format.enable": true,
  "tabnine.shortcut": "Tab",
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  },
  "codeLens.enable": true,
  "git.enableGutters": false
}
